{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "felipe-sa-factory"
		},
		"ls_sql_db_felipe_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sql_db_felipe'"
		},
		"ls_sql_learning_felipe_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_learning_felipe_sa'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=server-learning-felipe.database.windows.net;Initial Catalog=db-learning-felipe;User ID=felipe-sa"
		},
		"link_vault_felipe_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://key-learning-felipe.vault.azure.net/"
		},
		"lis_fpt_ibge_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "ftp.ibge.gov.br"
		},
		"ls_sql_db_felipe_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dllearningfelipe.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "felipe-sa-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sql_db_felipe_accountKey": {
						"value": "[parameters('ls_sql_db_felipe_accountKey')]"
					},
					"ls_sql_learning_felipe_sa_connectionString": {
						"value": "[parameters('ls_sql_learning_felipe_sa_connectionString')]"
					},
					"link_vault_felipe_properties_typeProperties_baseUrl": {
						"value": "[parameters('link_vault_felipe_properties_typeProperties_baseUrl')]"
					},
					"lis_fpt_ibge_properties_typeProperties_host": {
						"value": "[parameters('lis_fpt_ibge_properties_typeProperties_host')]"
					},
					"ls_sql_db_felipe_properties_typeProperties_url": {
						"value": "[parameters('ls_sql_db_felipe_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}